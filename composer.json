{
    "name": "mutant/items-manager",
    "description": "MuOnline library that support managing character items",
    "type": "library",
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.12.18",
        "phpstan/phpstan-phpunit": "^0.12.6",
        "phpunit/phpunit": "8.2.*",
        "povils/phpmnd": "^2.0",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mutant\\ItemsManager\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mutant\\Tests\\ItemsManager\\": "tests/"
        }
    }
}
